# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[*.coffee]
indent_style = space
indent_size = 2

[*.js]
indent_style = space
indent_size = 2

[*.yaml]
indent_style = space
indent_size = 2

[*.hyle]
indent_style = space
indent_size = 2
